{
  "name": "@projectwallace/css-analyzer",
  "version": "4.0.0-0",
  "author": "Bart Veneman",
  "repository": {
    "type": "git",
    "url": "https://github.com/projectwallace/css-analyzer"
  },
  "homepage": "https://www.projectwallace.com/oss",
  "issues": "https://github.com/projectwallace/css-analyzer/issues",
  "license": "MIT",
  "engines": {
    "node": ">=10.0"
  },
  "keywords": [
    "projectwallace",
    "wallace",
    "css",
    "stats",
    "statistics",
    "analytics",
    "cssstats",
    "parker",
    "performance",
    "styleguide",
    "metrics",
    "selector",
    "value",
    "declaration",
    "property",
    "rule",
    "atrule"
  ],
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "nyc ava -v"
  },
  "prettier": {
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "useTabs": false
        }
      }
    ]
  },
  "dependencies": {
    "color-sorter": "^4.0.1",
    "css-selector-parser": "^1.4.1",
    "css-shorthand-expand": "^1.2.0",
    "css-tree": "^1.0.0-alpha.39",
    "css-unit-sort": "^3.3.1",
    "gzip-size": "^5.1.1",
    "is-vendor-prefixed": "^3.3.1",
    "postcss-values-parser": "^3.1.1",
    "specificity": "^0.4.1",
    "split-lines": "^2.0.0",
    "string-natural-compare": "^3.0.1",
    "tinycolor2": "^1.4.1",
    "vendor-prefixes": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^3.5.0",
    "coveralls": "^3.0.9",
    "nyc": "^15.0.0",
    "prettier": "^2.0.4"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "src/node_modules/**/test.js",
      "src/node_modules/**/test/*.js"
    ]
  }
}
